<launch>
  <node name="pgd_start" pkg="ros_verilook" type="pgd_start.sh" />

  <node pkg="ros_verilook"
        type="EnrollFaceFromROSTopicNode"
        name="EnrollFaceFromROSTopic">
        <param name="image_transport" value="compressed" />
  </node>

  <node pkg="ros_verilook"
        type="identify_face_node.py"
        name="identify_face_node" />

  <test test-name="test_save_and_identify"
        pkg="ros_verilook"
        type="test_save_and_identify.py"
        name="test_save_and_identify" />
</launch>
